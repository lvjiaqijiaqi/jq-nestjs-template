{
  "name": "jq-project-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=\"src/.*\\.spec\\.ts$\" --testTimeout=30000",
    "test:unit:watch": "jest --testPathPattern=\"src/.*\\.spec\\.ts$\" --watch",
    "test:unit:cov": "jest --testPathPattern=\"src/.*\\.spec\\.ts$\" --coverage",
    "test:integration": "jest --testPathPattern=\"test/integration/.*\\.test\\.ts$\" --testTimeout=60000",
    "test:integration:watch": "jest --testPathPattern=\"test/integration/.*\\.test\\.ts$\" --watch",
    "test:e2e:run": "jest --testPathPattern=\"test/e2e/.*\\.e2e-spec\\.ts$\" --testTimeout=120000 --runInBand",
    "test:e2e:watch": "jest --testPathPattern=\"test/e2e/.*\\.e2e-spec\\.ts$\" --watch --runInBand",
    "test:all": "jest --testTimeout=60000 --detectOpenHandles --forceExit",
    "test:all:cov": "jest --coverage --testTimeout=60000 --detectOpenHandles --forceExit",
    "test:ci": "jest --ci --coverage --testTimeout=60000 --detectOpenHandles --forceExit --passWithNoTests",
    "test:clear": "jest --clearCache",
    "test:setup": "./scripts/test.sh setup",
    "test:clean": "./scripts/test.sh clean",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- -d data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- -d data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- -d data-source.ts migration:revert",
    "migration:show": "npm run typeorm -- -d data-source.ts migration:show",
    "schema:sync": "npm run typeorm -- -d data-source.ts schema:sync",
    "schema:drop": "npm run typeorm -- -d data-source.ts schema:drop",
    "seed:auth": "ts-node src/scripts/seed-auth.ts",
    "prepare": "husky install",
    "commit": "git-cz",
    "check": "npm run lint && npm run test:unit",
    "check:all": "npm run lint && npm run test:all",
    "docker:build": "docker build -t jq-nestjs-app .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t jq-nestjs-app:dev .",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:dev": "./scripts/deploy.sh dev",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:prod": "./scripts/deploy.sh prod"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cache-manager": "^7.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "express-prometheus-middleware": "^1.2.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "mysql2": "^3.14.2",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-wechat": "^2.0.4",
    "prom-client": "^13.2.0",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.5",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^6.0.0",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.1.2",
    "nock": "^14.0.7",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
