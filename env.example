# 应用配置
NODE_ENV=development
PORT=3000
APP_NAME=jq-project-template
APP_VERSION=1.0.0

# 数据库配置
DB_TYPE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=your_username
DB_PASSWORD=your_password
DB_NAME=your_database_name
DB_SYNCHRONIZE=false
DB_LOGGING=false
DB_SSL=false
DB_SSL_REJECT_UNAUTHORIZED=true
DB_MAX_CONNECTIONS=10
DB_MIN_CONNECTIONS=1
DB_ACQUIRE_TIMEOUT=60000
DB_IDLE_TIMEOUT=600000

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-at-least-32-characters-long
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production-at-least-32-characters-long
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d
JWT_ISSUER=jq-project-template
JWT_AUDIENCE=jq-project-template-users

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# CORS配置
CORS_ENABLED=true
CORS_CREDENTIALS=true

# API配置
API_PREFIX=api
API_VERSION=v1

# 安全配置
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=100
BODY_PARSER_LIMIT=10mb
BODY_PARSER_PARAMETER_LIMIT=1000

# 密码策略
PASSWORD_MIN_LENGTH=8
PASSWORD_REQUIRE_UPPERCASE=true
PASSWORD_REQUIRE_LOWERCASE=true
PASSWORD_REQUIRE_NUMBERS=true
PASSWORD_REQUIRE_SYMBOLS=false

# IP过滤（可选，多个IP用逗号分隔）
IP_WHITELIST=
IP_BLACKLIST=

# 会话安全
MAX_CONCURRENT_SESSIONS=5
SESSION_TIMEOUT=3600

# 邮件配置
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USER=your_email@gmail.com
MAIL_PASSWORD=your_app_password
MAIL_FROM=noreply@yourapp.com

# 日志配置
LOG_LEVEL=debug

# 云存储配置（可选）
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET_NAME=

# 第三方服务配置（可选）
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
WECHAT_APP_ID=
WECHAT_APP_SECRET=

# 监控配置（可选）
SENTRY_DSN= 